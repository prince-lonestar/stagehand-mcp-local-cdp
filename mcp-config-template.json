{
  "mcpServers": {
    "stagehand": {
      "command": "node",
      "args": [
        "/path/to/stagehand-mcp-local-cdp/cli.js"
      ],
      "env": {
        "OPENAI_API_KEY": "your-openai-api-key-here",
        "STAGEHAND_MODEL": "gpt-4o",
        "STAGEHAND_ENV": "LOCAL",
        "HEADLESS": "false",
        "CDP_URL": "http://localhost:9222"
      }
    }
  }
}
